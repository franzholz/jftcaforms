{
  "name": "jambagecom/jftcaforms",
  "type": "typo3-cms-extension",
  "description": "Provides additional TCA Forms for the backend of the TYPO3 CMS. At this moment these forms are supported: Slider",
  "keywords": [
    "TYPO3",
    "extension",
    "tca",
    "tce"
  ],
  "support": {
    "issues": "https://github.com/franzholz/jftcaforms/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    },
    {
      "name": "JÃ¼rgen Furrer",
      "role": "Developer"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "php": ">=5.5.0,<7.3.99",
    "typo3/cms-core": ">=7.6.0,<8.7.99"
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\Jftcaforms\\": "Classes"
    },
    "classmap": [
      "Classes",
      "lib"
    ]
  },
  "replace": {
    "jftcaforms": "self.version",
    "typo3-ter/jftcaforms": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "jftcaforms"
    }
  }
}
